  var width = 900, 
    height = 480;  

  var path = d3.geo.path() ;  

  svg = d3.select("body").append("svg")   
    .attr("width", width)
    .attr("height", height);

  d3.json("paris.geojson", function(json) {
    svg.selectAll("path")   
      .data(json.features)  
      .enter()
      .append("path") 
      .attr("d", path)
  });
